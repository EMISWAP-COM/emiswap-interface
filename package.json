  "scripts": {
    "start": "cross-env PORT=4202 react-scripts start",
    "build": "react-scripts build && REACTBUILDVERSION=$(date +%Y-%m-%d)-$(date +%T) && sed -i -- 's/%REACTBUILDVERSION%/'$REACTBUILDVERSION'/g' build/docs/version.txt && echo React Build Version = $REACTBUILDVERSION",
    "build:stage": "rm -R -f ./build && env-cmd -f .env.staging react-scripts build && REACTBUILDVERSION=Stage_$(date +%Y-%m-%d)-$(date +%T) && sed -i -- 's/%REACTBUILDVERSION%/'$REACTBUILDVERSION'/g' build/docs/version.txt && echo React Build Version = $REACTBUILDVERSION",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint '{cypress,src}/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix",
    "format": "prettier 'src/**/*.{js,jsx,yml,yaml,ts,tsx,md,graphql,mdx}' --write",
    "integration-test": "start-server-and-test 'serve build -l 3000' http://localhost:3000 'cypress run'",
    "precommit": "lint-staged"
  },
